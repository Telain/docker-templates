<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>True</Beta>
  <Name>danbooru</Name>
  <Description>
  </Description>
  <Overview>
  </Overview>
  <Registry>https://registry.hub.docker.com/u/telain/danbooru/</Registry>
  <GitHub>https://github.com/telain/danbooru</GitHub>
  <Repository>telain/danbooru</Repository>
  <Project>https://github.com/r888888888/danbooru</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>80</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>RUBY_VERSION</Name>
      <Value>2.1.5</Value>
    </Variable>
    <Variable>
      <Name>GITHUB_INSTALL</Name>
      <Value>https://raw.githubusercontent.com/Telain/danbooru/master</Value>
    </Variable>
    <Variable>
      <Name>POSTGRESQL_VERSION</Name>
      <Value>9.1</Value>
    </Variable>
    <Variable>
      <Name>GITHUB_SOURCE</Name>
      <Value>Telain/danbooru</Value>
    </Variable>
    <Variable>
      <Name>HOSTNAME</Name>
      <Value>TOWER</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/danbooru/data</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/danbooru/var/www</HostDir>
      <ContainerDir>/var/www</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Containers>